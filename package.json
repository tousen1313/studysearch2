{
  "name": "engage-search2-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "api": "node ./mocks/merge.js && json-server --watch --routes ./mocks/routes.json ./mocks/db.json --port 3004",
    "lint": "next lint",
    "lint:ci": "next lint --max-warnings=0",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:storybook": "test-storybook --url http://localhost:6006",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build --quiet",
    "storybook:serve": "npx http-server storybook-static -a localhost -p 6006 -s",
    "storybook:test": "npx wait-on http://localhost:6006 && npm run test:storybook",
    "storybook:ci": "npm run storybook:build && npx concurrently -k -s first \"npm run storybook:serve\" \"npm run storybook:test\""
  },
  "dependencies": {
    "next": "13.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.0.26",
    "@storybook/addon-coverage": "^0.0.8",
    "@storybook/addon-essentials": "^7.0.25",
    "@storybook/addon-interactions": "^7.0.25",
    "@storybook/addon-links": "^7.0.25",
    "@storybook/blocks": "^7.0.25",
    "@storybook/jest": "^0.1.0",
    "@storybook/nextjs": "^7.0.25",
    "@storybook/react": "^7.0.25",
    "@storybook/test-runner": "^0.11.0",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@svgr/webpack": "^8.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "^20.4.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.41.0",
    "eslint-config-next": "^13.4.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.6.1",
    "jest-html-reporters": "^3.1.4",
    "json-server": "^0.17.3",
    "postcss": "^8.4.25",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "react-paginate": "^8.2.0",
    "storybook": "^7.0.25",
    "ts-case-convert": "^2.0.2",
    "typescript": "^5.1.6"
  }
}
